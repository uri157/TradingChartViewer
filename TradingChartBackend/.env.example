# ===== Runtime (non-sensitive) =====
API_PORT=8080                 # API port inside the container
DUCKDB_PATH=/data/market.duckdb
EXCHANGE=binance
LIVE=1
LIVE_SYMBOLS=BTCUSDT,ETHUSDT
LIVE_INTERVALS=1m
LOG_LEVEL=info                # trace|debug|info|warn|error

# ===== CORS =====
CORS_ENABLE=1
# Frontend origin (ONE, or CSV if your backend already supports it)
# DEV:  http://localhost:3000
# PROD: https://www.tradingchart.ink
CORS_ORIGIN=

# ===== Dev-only (bind mount ownership) =====
UID=1000
GID=1000
